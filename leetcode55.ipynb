   "source": [
    "def canJump(nums: list[int]) -> bool:\n",
    "    \n",
    "    goal = len(nums) - 1\n",
    "    \n",
    "    for i in range(len(nums) - 2, -1, -1):\n",
    "        if i + nums[i] >= goal:\n",
    "            goal = i\n",
    "            \n",
    "    return goal == 0"
   ]
  }
