   "source": [
    "def lengthOfLastWord(s: str) -> int:\n",
    "    \n",
    "    s = s.strip()\n",
    "    \n",
    "    if not s:\n",
    "        return 0\n",
    "        \n",
    "    last_space_index = s.rfind(' ')\n",
    "    \n",
    "    return len(s) - 1 - last_space_index"
   ]
  }
